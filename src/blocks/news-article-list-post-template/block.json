{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "news/article-list-post-template",
  "version": "1.0.0",
  "title": "News Article List Post Template",
  "category": "news",
  "icon": "list",
  "description": "A customizable template for list article positions in the Front Layout block",
  "keywords": ["news", "articles", "template", "post", "list", "front"],
  "textdomain": "news",
  "editorScript": "file:./index.js",
  "style": "file:./style.css",
  "render": "file:./render.php",
  "attributes": {
    "postId": {
      "type": "number"
    },
    "postType": {
      "type": "string",
      "default": "news"
    },
    "position": {
      "type": "string",
      "default": "list"
    }
  },
  "templateLock": false,
  "parent": ["news/front-layout"],
  "usesContext": ["news/postId", "news/postType", "news/position", "postId", "postType"],
  "providesContext": {
    "news/postId": "postId",
    "news/postType": "postType",
    "news/position": "position",
    "postId": "postId",
    "postType": "postType"
  },
  "supports": {
    "reusable": false,
    "html": false,
    "align": true,
    "layout": true,
    "spacing": {
      "padding": true,
      "margin": true
    },
    "innerBlocks": {
    }
  }
}
